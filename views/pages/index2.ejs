<!DOCTYPE html>
<html>
    <head>
        <% include ../partials/header %>
    </head>

    <body>

        <div class="wrapper">
            <!-- Sidebar Holder -->
            <nav id="sidebar">
                <div class="sidebar-header">
                    <h3>Proyecto IRSSAS</h3>
                </div>

                <ul class="list-unstyled components mainmenu">
                    <p id="myp">Indicadores</p>
                    <li class="active"><a onclick="changeComponent('Componente',1);">Saneamiento</a>
                    <ul class="components submenu">
                    <li><a onclick="changeComponent('SubComponente',1);">Gestión de Aguas Residuales</a></li>
                    <li><a onclick="changeComponent('SubComponente',2);">Gestión de Residuos Sólidos</a></li>
                    </ul></li>

                    <li class="active"><a onclick="changeComponent('Componente',2);" >Gestión del Recurso Hídrico</a>
                    <ul class="components submenu">
                    <li><a onclick="changeComponent('SubComponente',3);">Gestión de Protección del Recurso Hídrico</a></li>
                    <li><a onclick="changeComponent('SubComponente',4);">Calidad de Agua Potable</a></li>
                    <li><a onclick="changeComponent('SubComponente',5);">Gestión Administrativo/Financiero</a></li>
                    </ul></li>

                    <li class="active"><a a onclick="changeComponent('Componente',3);" >Índice de Desarrollo</a></li>
                    <li class="active"><a a onclick="changeComponent('Componente',4);">Educación</a></li>
                    <li class="active"><a a onclick="changeComponent('Componente',5);">Fenómenos Naturales</a></li>
                    <li class="active laclase"><a a onclick="changeComponent('IRSSAS',0);">IRSSAs</a></li>

                    
                </ul>

                
            </nav>

            <!-- Page Content Holder -->
            <div id="content">

                <nav class="navbar navbar-default">
                    <div class="container-fluid">

                        <div class="navbar-header">
                            <button type="button" id="sidebarCollapse" class="btn btn-info navbar-btn">
                                <i class="glyphicon glyphicon-leaf"></i>
                                <span></span>
                            </button>
                        </div>

                        <div style="float: right;">
                            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                                <form class="form-inline" method="post">
                                    <div class="form-group">
                                        <input type="text" class="form-control" id="usr" name="usr" placeholder="Usuario">
                                    </div>
                                    <div class="form-group">
                                        <input type="password" class="form-control" id="pwd" name="pwd" placeholder="Contraseña">
                                    </div>
                                    <button type="submit" class="btn btn-info navbar-btn">Ingresar</button>
                                    <p style="color: #f00;"><%= error%></p>
                                </form>
                            </div>
                                
                        </div>
                    </div>
                    <div id="map">
                    </div>
                    <% include ../partials/riesgo %>
                </nav>
            </div>
        </div>


        <!-- jQuery CDN -->
        <script src="https://code.jquery.com/jquery-1.12.0.min.js"></script>
        <!-- Bootstrap Js CDN -->
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <!-- jQuery Custom Scroller CDN -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/malihu-custom-scrollbar-plugin/3.1.5/jquery.mCustomScrollbar.concat.min.js"></script>

        <script type="text/javascript">
            $(document).ready(function () {
               

                $('#sidebarCollapse').on('click', function () {
                    $('#sidebar, #content').toggleClass('active');
                    $('.collapse.in').toggleClass('in');
                    $('a[aria-expanded=true]').attr('aria-expanded', 'false');
                });

                $("#divrisk").click(function(){
                     $("#divrisk").toggle("slow","linear");
                });

                $("#buttonrisk").click(function(){
                     $("#divrisk").toggle("slow","linear");
                });


            });
        </script>
        <script src="scripts/map.js"></script>
        <script src="scripts/script.js"></script>
    </body>
</html>
